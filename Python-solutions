#def row_sum_odd_numbers(n):
    return n**3

8kyu
#Opposite number
def opposite(number):
      return -1 * number
      

#Is the string uppercase?
def is_uppercase(inp):
    if any(x.islower() for x in inp):
        return False
    else: 
        return True
