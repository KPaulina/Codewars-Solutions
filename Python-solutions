7 kyu problems
#Form The Minimum
def min_value(digits):
    uniqueDigits = sorted(list(set(digits)))  
    return int(''.join(list(map(str,uniqueDigits))))

#Square Every Digit
def square_digits(num):
    suma = ''.join(str(int(i)**2) for i in str(num))
    return int(suma)
    
